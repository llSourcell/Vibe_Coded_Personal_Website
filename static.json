{
  "root": ".next",
  "clean_urls": true,
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
    }
  },
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/": {
      "origin": "${NEXT_PUBLIC_URL}"
    }
  },
  "redirects": {
    "/": {
      "url": "https://www.sirajraval.com/",
      "status": 301
    }
  }
} 